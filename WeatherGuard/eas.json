{
  "cli": {
    "version": ">= 14.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "release": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "debug": {
      "developmentClient": true
    },
    "internal": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
